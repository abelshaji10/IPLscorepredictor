yer\n","    keras.layers.Dense(512, activation='relu'),  # Hidden layer with 512 units and ReLU activation\n","    keras.layers.Dense(216, activation='relu'),  # Hidden layer with 216 units and ReLU activation\n","    keras.layers.Dense(1, activation='linear')  # Output layer with linear activation for regression\n","])\n","\n","# Compile the model with Huber loss\n","huber_loss = tf.keras.losses.Huber(delta=1.0)  # You can adjust the 'delta' parameter as needed\n","model.compile(optimizer='adam', loss=huber_loss)  # Use Huber loss for regression\n"],"metadata":{"id":"6TXa1-7J8GsO","executionInfo":{"status":"ok","timestamp":1730888676779,"user_tz":-330,"elapsed":472,"user":{"displayName":"Abel Shaji","userId":"06871148618311874021"}}},"execution_count":22,"outputs":[]},{"cell_type":"markdown","source":["## Model Training"],"metadata":{"id":"0Rj154pR8N8d"}},{"cell_type":"code","source":["# Train the model\n","model.fit(X_train_scaled, y_train, epochs=50, batch_size=64, validation_data=(X_test_scaled, y_test))\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"bFS0y2BY8P3u","executionInfo":{"status":"ok","timestamp":1730889005981,"user_tz":-330,"elapsed":324844,"user":{"displayName":"Abel Shaji","userId":"06871148618311874021"}},"outputId":"a739032b-e4c3-424f-f0c7-d46de94efc03"},"execution_count":23,"outputs":[{"output_type":"stream","name":"stdout","text":["Epoch 1/50\n","\u001b[1m832/832\u001b[0m \u001b[32mâ”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”\u001b[0m\u001b[37m\u001b[0m \u001b[1m7s\u001b[0m 7ms/step - loss: 54.6743 - val_loss: 22.2308\n","Epoch 2/50\n","\u001b[1m832/832\u001b[0m \u001b[32mâ”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”\u001b[0m\u001b[37m\u001b[0m \u001b[1m5s\u001b[0m 5ms/step - loss: 22.4411 - val_loss: 22.2965\n","Epoch 3/50\n","\u001b[1m832/832\u001b[0m \u001b[32mâ”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”\u001b[0m\u001b[37m\u001b[0m \u001b[1m4s\u001b[0m 5ms/step - loss: 22.3717 - val_loss: 22.0645\n","Epoch 4/50\n","\u001b[1m832/832\u